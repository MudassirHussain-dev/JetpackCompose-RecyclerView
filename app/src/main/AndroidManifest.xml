<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jetpackcomposestarting">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="com.example.jetpackcomposestarting.data.di.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JetPackComposeStarting">
        <activity android:name=".uii.RecyclerviewActivity" />

        <activity
            android:name=".uii.RetrofitActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.JetPackComposeStarting.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".uii.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.JetPackComposeStarting.NoActionBar">

        </activity>
    </application>

</manifest>